{
  "name": "sample-http-server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "tsup",
    "build:start": "pnpm build && pnpm start",
    "dev": "tsup --watch  --onSuccess \"node dist/index.js\"",
    "info:routes": "fastify print-routes ./dist/index.js",
    "info:swagger": "fastify generate-swagger ./dist/index.js",
    "clean": "rimraf node_modules && rm package-lock.json",
    "clean:i": "pnpm clean && pnpm i"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/request-context": "^6.0.1",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.0.1",
    "close-with-grace": "^2.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.1",
    "nanoid": "^5.0.7",
    "openapi-types": "^12.1.3",
    "pino": "^9.4.0",
    "rimraf": "^6.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^2.0.0",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.4.0",
    "tsup": "^8.2.4",
    "typescript": "^5.2.2"
  }
}